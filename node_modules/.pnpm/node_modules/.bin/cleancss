#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/naoko/repo/01study/baby_monitor/node_modules/.pnpm/clean-css@3.4.28/node_modules/clean-css/bin/node_modules:/Users/naoko/repo/01study/baby_monitor/node_modules/.pnpm/clean-css@3.4.28/node_modules/clean-css/node_modules:/Users/naoko/repo/01study/baby_monitor/node_modules/.pnpm/clean-css@3.4.28/node_modules:/Users/naoko/repo/01study/baby_monitor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/naoko/repo/01study/baby_monitor/node_modules/.pnpm/clean-css@3.4.28/node_modules/clean-css/bin/node_modules:/Users/naoko/repo/01study/baby_monitor/node_modules/.pnpm/clean-css@3.4.28/node_modules/clean-css/node_modules:/Users/naoko/repo/01study/baby_monitor/node_modules/.pnpm/clean-css@3.4.28/node_modules:/Users/naoko/repo/01study/baby_monitor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../clean-css/bin/cleancss" "$@"
else
  exec node  "$basedir/../clean-css/bin/cleancss" "$@"
fi
